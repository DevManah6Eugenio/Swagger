paths: 
  '/cliente':  
    post: 
      summary: Cadastra cliente 
      description: Cadastra um novo cliente 
      consumes:  
        - application/json 
      produces:  
        - application/json 
      operationId: cadastraCliente 
      parameters:
        - in: body 
          name: cliente_ 
          schema: 
            type: object 
            properties: 
              titular: 
                type: string
                example: Manasses Eugenio
      responses:
        201:
          description: Cadastro efetuado com sucesso
          schema: 
            type: object 
            properties: 
              titular: 
                type: string
                example: Manasses Eugenio 
        400:
          description: Reguisição invalida
        500:
          description: Erro interno no servidor
